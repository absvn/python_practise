    "def sockMerchant(n, ar):\n",
    "    count = 0\n",
    "    ar.sort()\n",
    "    ar.append('#')\n",
    "    i = 0\n",
    "    while i<n:\n",
    "        if ar[i]==ar[i+1]:\n",
    "            count = count+1\n",
    "            i+=2\n",
    "        else:\n",
    "            i+=1\n",
    "    return count"
   ]

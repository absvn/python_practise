    "def substring(word):\n",
    "    word = list(set(word))\n",
    "    word = ''.join(word)\n",
    "    sub_string = []\n",
    "    l = len(word)\n",
    "    for i in range(l):\n",
    "        for k in range(i+1,l+1):         \n",
    "            sub_string.append(word[i:k])\n",
    "    print(len(sub_string))\n",
    "\n",
    "substring('aabc')"
   ]
  }

   "source": [
    "def pageCount(n, p):\n",
    "    a = (p-n)//2\n",
    "    b = n//2\n",
    "    return a if a<b else b\n",
    "\n",
    "p = int(input())\n",
    "n = int(input())\n",
    "print(pageCount(n,p))"
   ]

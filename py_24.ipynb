    "import itertools\n",
    "n = [1, 2, 3]\n",
    "t = 3\n",
    "r = [total for i in range(len(n), 0, -1) \n",
    "          for total in itertools.combinations(n, i) \n",
    "          if sum(total) == t]\n",
    "print(r)"
   ]
